{
  "name": "cciemaster",
  "version": "1.0.0",
  "description": "CCIE培训网站，静态部署版本",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "BUILD_STATIC=true next build",
    "build:windows": "set BUILD_STATIC=true && next build",
    "preview": "npm run build && npx serve out",
    "deploy:prepare": "npm run build",
    "deploy:namecheap": "npm run build && node deploy-namecheap.js",
    "clean": "rm -rf .next out",
    "clean:windows": "rmdir /s .next out"
  },
  "keywords": [
    "ccie",
    "training",
    "network",
    "education",
    "static-site"
  ],
  "author": "CCIE培训中心",
  "license": "ISC",
  "dependencies": {
    "express": "^5.1.0",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "ftp": "^0.3.10",
    "serve": "^14.2.1"
  }
}
